package fb_assignment.array;

public class LargestAndSmallestNumber {

	public static void main(String[] args) {

		int array[] = {2, 3, 5, 6, 5};
		int smaller =array[0]	, larger =array[0];
		
		for(int i=0; i<array.length; i++) {
			
			if(smaller  > array[i])
				smaller = array[i];
			if(larger < array[i])
				larger = array[i];
		}
		
		System.out.println("SMaller Number: " + smaller + "And Larger Number: "+ larger);
		
	}

}
