package com.streams;

import java.util.Arrays;

public class AshmiToken {

	public static void main(String[] args) {

		int arr[] = { 1, 2, 3, 2, 4, 6, 8, 5, 7, 12, 8, 4, 16, 18, 3, 2, 4, 6, 8, 7 };
		
		Arrays.asList(arr).stream()
			.filter(m -> m %2 == 0)
			.forEach(System.out :: println);
			
		
	}

}
